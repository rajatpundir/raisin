<% @page_title = "Edit Test"%>
<div class="center page-header">
	<h2>Edit Test</h2>
</div>
<%= form_for(@objective_test,:url=> objective_test_path(@objective_test),:method => 'patch', :html => {:class => 'form-horizontal', :role => 'form'}) do |f| %>
<%= hidden_field_tag :floor_id, @floor_id %>
<div class="form-group">
  <%= f.label :title, class: 'col-sm-2 control-label' %>
  <div class="col-sm-8">
    <%= f.text_field :title, class: 'form-control', placeholder: 'Enter title', autofocus: true %>
  </div>
</div>
<div class="form-group">
  <%= f.label :start_time, class: 'col-sm-2 control-label' %>
  <div class="col-sm-8">
    <%= f.text_field :start_time, class: 'form-control', placeholder: 'Input start time in seconds from now', autofocus: true %>
  </div>
</div>
<div class="form-group">
  <%= f.label :end_time, class: 'col-sm-2 control-label' %>
  <div class="col-sm-8">
    <%= f.text_field :end_time, class: 'form-control', placeholder: 'Input end time in seconds from now', autofocus: true %>
  </div>
</div>
<div class="form-group">
  <%= f.label :test_duration, class: 'col-sm-2 control-label' %>
  <div class="col-sm-8">
    <%= f.text_field :test_duration, class: 'form-control', placeholder: 'Input test test duration in seconds', autofocus: true %>
  </div>
</div>
<div class="form-group">
	<div class="col-sm-offset-2 col-sm-10">
		<%= f.submit "Update",class: 'btn btn-primary btn-lg' %>
	</div>
</div>
<% end %>

